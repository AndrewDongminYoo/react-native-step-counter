{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build:android": {
      "inputs": [
        "package.json",
        "android",
        "!android/build",
        "src/*.ts",
        "src/*.tsx"
      ],
      "outputs": []
    },
    "build:ios": {
      "inputs": ["package.json", "*.podspec", "ios", "src/*.ts", "src/*.tsx"],
      "outputs": []
    }
  }
}
