{
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "semi": true,
  "quoteProps": "consistent",
  "trailingComma": "es5",
  "printWidth": 120,
  "overrides": [
    {
      "files": ["**/*.ts", "**/*.tsx", "**/*.js", "**/*.jsx"],
      "options": {
        "parser": "babel-ts"
      }
    }
  ]
}
